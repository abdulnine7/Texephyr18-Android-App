                    />

                <TextView
                    android:id="@+id/nancy_number"
                    android:text="+91 9763881559"
                    android:autoLink="phone"
                    android:linksClickable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:layout_below="@id/nancy_name"
                    />

                <TextView
                    android:id="@+id/nancy_email"
                    android:text="nancydas12@gmail.com"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:layout_below="@id/nancy_number"
                    />
            </RelativeLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/chinmay_card"
            android:layout_below="@id/nancy_card"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            card_view:cardCornerRadius="1dp">


            <RelativeLayout
                android:layout_margin="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/chinmay_name"
                    android:text="Chinmay Rane"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="20sp"
                    />

                <TextView
                    android:autoLink="phone"
                    android:linksClickable="true"
                    android:id="@+id/chinmay_number"
                    android:text="+91 9503425810"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:layout_below="@id/chinmay_name"
                    />

                <TextView
                    android:id="@+id/chinmay_email"
                    android:text="chinmayurane@gmail.com"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"
                    android:layout_below="@id/chinmay_number"
                    />
            </RelativeLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:id="@+id/kushal_card"
            android:layout_below="@id/chinmay_card"
            android:layout_gravity="center"
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
